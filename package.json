{
  "name": "talenvoice",
  "version": "1.0.0",
  "description": "TalentVoice - AI-powered conversational recruiter with Agora integration",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm run dev",
    "install-all": "npm install && cd client && npm install",
    "test-email": "node test-email.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "multer": "^1.4.5-lts.1",
    "pdf-parse": "^1.1.1",
    "mammoth": "^1.6.0",
    "openai": "^4.20.1",
    "dotenv": "^16.3.1",
    "agora-rtc-sdk-ng": "^4.19.0",
    "agora-access-token": "^2.0.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2"
  }
}
